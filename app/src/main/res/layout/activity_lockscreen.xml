<?xml version="1.0" encoding="utf-8"?>
<com.haokan.hklockscreen.lockscreen.CV_ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollview"
    android:overScrollMode="never"
    android:scrollbars="none"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/rootview"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/lockscreen_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>

        <com.haokan.hklockscreen.lockscreen.CV_RecommendPage_LockScreen
            android:id="@+id/cv_recommendpage"
            android:layout_below="@id/lockscreen_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </com.haokan.hklockscreen.lockscreen.CV_RecommendPage_LockScreen>
    </RelativeLayout>
</com.haokan.hklockscreen.lockscreen.CV_ScrollView>
